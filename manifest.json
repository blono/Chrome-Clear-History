{
  "manifest_version": 3,
  "name": "Clear History",
  "description": "Clear chrome browser history on startup",
  "version": "1.0.0",
  "homepage_url": "https://github.com/blono/Chrome-Clear-History",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "history", "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  }
}
